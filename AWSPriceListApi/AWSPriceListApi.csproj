<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <RootNamespace>BAMCIS.AWSPriceListApi</RootNamespace>
    <Authors>Michael Haken</Authors>
    <Company>bamcis.io</Company>
    <Description>Provides implementations for the AWS Price List API.</Description>
    <PackageTags>AWS PriceList Lambda</PackageTags>
    <RepositoryType>Git</RepositoryType>
    <PackageProjectUrl>https://github.com/bamcis-io/AWSPriceListApi</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bamcis-io/AWSPriceListApi</RepositoryUrl>
    <Copyright>bamcis.io</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/bamcis-io/AWSPriceListApi/master/LICENSE</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NetStandardImplicitPackageVersion>1.6</NetStandardImplicitPackageVersion>
    <Version>2.2.2</Version>
    <PackageReleaseNotes>Made all dictionary key comparisons case insensitive.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

</Project>
